{
  "name": "sporty-galaxy-backend",
  "version": "1.0.0",
  "description": "sport-ecommerce",
  "main": "app.ts",
  "scripts": {
    "dev": "console-ninja node --watch dist/app.js",
    "run-migrations": "npx prisma db push",
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "console-ninja node --watch dist/app.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\"  \"console-ninja node --watch dist/app.js\"",
    "seeder": "npx ts-node seeder.ts"
  },
  "_moduleAliases": {
    "@common": "dist/common",
    "@controllers": "dist/controllers",
    "@helpers": "dist/helpers",
    "@lib": "dist/lib",
    "@middleware": "dist/middleware",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@types": "dist/types",
    "@utils": "dist/utils",
    "@constants": "dist/constants.ts"
  },
  "author": "Gberaese Erons Austine",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.16",
    "bcrypt": "^5.1.1",
    "concurrently": "^7.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "prisma": "^5.11.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "console-log-colors": "^0.4.0"
  }
}
